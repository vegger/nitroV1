<{{tag}} class="a-checkbox{% if state %} state-a-checkbox--{{state}}{% endif %}">
<input type="checkbox" id="{{inputId}}" class="a-checkbox__input" name="{{inputName}}" value="{{inputValue}}"{% if checked %} checked{% endif %}>
{% if label %}<label class="a-checkbox__label" for="{{inputId}}">{{label}}</label>{% endif %}
</{{tag}}>
